p1.
begin
n = 12
previous = 0
current = 0
index = 1
while(index <= n){temp = current;current = current + previous; previous = temp; index+= 1}
end

p2.
begin
n = 8
val = 1
current = 1
while(counter <= n){val = val * counter; counter = counter + 1; print(val)}
end

p3.
begin
n = 100
odd = 0
even = 0
index = 1
while (index <= n){if(index % 2 == 0){even = even + index}else{odd = odd + index}}
index = index + 1
print(odd)
print(even)
print(odd + even)
end

